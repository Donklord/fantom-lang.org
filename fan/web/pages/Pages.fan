using afIoc::Inject
using afBedSheet::BedSheetServer

const mixin IndexPage		: FangPage {

	@Inject abstract BedSheetServer bedServer

	static const Str[] tagLines := [
		"Intelligent Programming",	// title's so nice, gotta use it twice!
		"Intelligent Programming",
		"JVM Programming Language",
		"Programming's Best Kept Secret",
		"Don't Follow The Herd",
		"Next Generation Programming",
		"Because Pragmatism Wins the Day!",
	]

	abstract Str tagLine

	override Void beforeRender() {
		injector.injectMeta.withName("google-site-verification").withContent("h6sOCvoTYAGBVC6vuydKFtY3cmLsnumk6YPDNGVY6fE")
		injector.injectMeta.withName("msvalidate.01").withContent("EB7395B494B820668899AA0E750A4928")	// https://www.bing.com/webmaster/home/
		injector.injectMeta.withName("description").withContent("Fantom - The Intelligent JVM Programming Language")

		injector.injectScript(true).withScript(`etc/web-static/js/indexPage.js`.toFile.readAllStr)
		tagLine = tagLines.random
	}
}

const mixin DownloadPage	: FangPage {
	override Void beforeRender() {
		injector.injectMeta.withName("description").withContent("Downloads and installations for the Fantom programming language")
	}
}

const mixin IdePage 		: FangPage {
	override Void beforeRender() {
		injector.injectMeta.withName("description").withContent("IDEs for the Fantom programming language")
	}
}

const mixin CommunityPage	: FangPage {
	override Void beforeRender() {
